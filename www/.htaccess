php_value upload_max_filesize 200M
php_value post_max_size 200M
php_value memory_limit 256M

<IfModule mod_rewrite.c>

##
# Rewrite base
##
RewriteEngine On
RewriteBase /

##
# Rewrite pages
##
RewriteRule ^login/*$                  	/index.php?page=login&%{QUERY_STRING}  [NC,L]
RewriteRule ^signup/*$									/index.php?page=signup&%{QUERY_STRING}  [NC,L]

</IfModule>

ErrorDocument 404                  			/index.php?page=nopage&path=%{PATH_INFO}